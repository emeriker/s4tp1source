<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<TitledPane fx:id="tiledPaneLivre" animated="false" text="Titre" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controleur.FragmentLivre">
   <content>
      <AnchorPane minWidth="0.0">
         <children>
            <HBox alignment="CENTER" fillHeight="false" spacing="20.0">
               <children>
                  <VBox alignment="CENTER_LEFT" spacing="3.0">
                     <children>
                        <Label fx:id="labelNombreMot" text="Nombre de mot :" />
                        <Label fx:id="LabelNombreMotDistinct" text="Nombre de mot distinct :" />
                        <Label fx:id="labelMoyenneOccurence" text="Moyenne d'occurence par mot :" />
                        <Label fx:id="labelMotPlusNombreux" text="Mot le plus nombreux :" />
                        <Label fx:id="labelMotPlusRare" text="Mot le plus rare :" />
                        <Label fx:id="labelMotPlusLong" text="Mot le plus long :" />
                        <Label fx:id="labelMotPlusCourt" text="Mot le plus court :" />
                     </children>
                  </VBox>
                  <VBox alignment="TOP_RIGHT" minWidth="100.0" spacing="3.0">
                     <children>
                        <Text fx:id="texteNombreDeMot" strokeType="OUTSIDE" strokeWidth="0.0" text="0" />
                        <Text fx:id="texteNombreDeMotDisctinct" strokeType="OUTSIDE" strokeWidth="0.0" text="0" />
                        <Text fx:id="texteMoyenneOccurence" strokeType="OUTSIDE" strokeWidth="0.0" text="0" />
                        <Text fx:id="texteMotPlusNombreux" strokeType="OUTSIDE" strokeWidth="0.0" text="aucun" />
                        <Text fx:id="texteMotPlusRare" strokeType="OUTSIDE" strokeWidth="0.0" text="aucun" />
                        <Text fx:id="texteMotPlusLong" strokeType="OUTSIDE" strokeWidth="0.0" text="aucun" />
                        <Text fx:id="texteMotPlusCourt" strokeType="OUTSIDE" strokeWidth="0.0" text="aucun" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
         </children>
      </AnchorPane>
   </content>
</TitledPane>
